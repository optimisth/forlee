<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, admin_list_bulletin_post_videos_path(@list, @bulletin, post) %></td>
        <td><%= post.description %></td>
        <td><%= post.content %></td>
        <td><%= link_to 'Show', [:admin, @list, @bulletin, post] %></td>
        <td><%= link_to 'Edit', edit_admin_list_bulletin_post_path(@list, @bulletin, post) %></td>
        <td><%= link_to 'Destroy', [:admin, @list, @bulletin, post], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_admin_list_bulletin_post_path %>

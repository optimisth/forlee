<p id="notice"><%= notice %></p>

<h1>Listing Videos</h1>

<table>
  <thead>
    <tr>
      <th>Video url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= video.video_url %></td>
        <td><%= link_to 'Show', [:admin, @list, @bulletin, @post] %></td>
        <td><%= link_to 'Edit', edit_admin_list_bulletin_post_video_path(@list, @bulletin, @post, video) %></td>
        <td><%= link_to 'Destroy', [:admin, @list, @bulletin, @post], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video', new_admin_list_bulletin_post_video_path %>
